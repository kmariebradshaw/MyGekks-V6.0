              <div class="sign-in">
                <ul>
                  {% if customer %}
                  <li><a href="/account/">Your Account</a></li>
                  <li><a href="/account/addresses/">Addresses</a></li>
                  <li>{{ 'Logout' | customer_logout_link }}</li>
                  {% else %}
                  <li>{{ 'Login' | customer_login_link }}</li>
                  <li>{{ 'Register' | customer_register_link }}</li>
                  {% endif %}
                </ul>
