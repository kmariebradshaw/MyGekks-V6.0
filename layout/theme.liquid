              <ul class="mobile-nav" style="display: none;">
                {% for link in linklists.product-sub-nav.links %}                          	
                <li><a href="{{ link.url }}" onClick="ga('send', 'event', 'SubNav', 'Touch', 'Women-Product');">{{ link.title | escape }}</a></li>
                {% endfor %}
              </ul>
              {% elsif link.title == "men" %}
              <li class="collection-m"><a href="{{ link.url }}" onClick="ga('send', 'event', 'Nav', 'Click', 'Men');">{{ link.title | escape }}</a></li>
              <ul class="mobile-nav" style="display: none;">
                {% for link in linklists.product-sub-nav-men.links %}                          	
                <li><a href="{{ link.url }}" onClick="ga('send', 'event', 'SubNav', 'Touch', 'Men-Product');">{{ link.title | escape }}</a></li>
                {% endfor %}
              </ul>
              {% else %}
              <li><a href="{{ link.url }}">{{ link.title | escape }}</a></li>
              {% endif%}
