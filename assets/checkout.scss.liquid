.breadcrumb {
  margin: 0px;
  padding: 0px;
  list-style-type: none;
  width: 100%;
  max-width: 800px;
  counter-reset: breadcrumb;
  position: relative;
  margin-left: -10%;
}

.breadcrumb:before {
  position: absolute;
  top: 23px;
  left: 12.5%;
  content: '';
  width: 75%;
  height: 1px;
  background: black;
}
.breadcrumb .breadcrumb__item {
  float: left;
  padding-top: 60px;
  width: 25%;
  text-align: center;
  position: relative;
}
.breadcrumb .breadcrumb__item:after {
  counter-increment: breadcrumb;
  content: counter(breadcrumb);
  position: absolute;
  top: 0px;
  left: calc(50% - 23px);
  width: 40px;
  height: 40px;
  text-align: center;
  border: 3px solid black;
  border-radius: 50%;
  line-height: 40px;
  background: white;
}
.breadcrumb .breadcrumb__item--completed:after {
  background: black;
  color: white;
}

